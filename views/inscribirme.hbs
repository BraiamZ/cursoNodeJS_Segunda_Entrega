<link rel="stylesheet" href="/css/bootstrap.min.css"  type="text/css">
<script src="js/jquery.min.js"></script>
<script src="js/popper.min.js"></script>
<script src="js/bootstrap.min.js"></script>
{{{navBar}}}
<div class="container">
    <div class="col-6">
        <h3>Inscripcion a cursos</h3>
    </div>
    <div>
        <form action="/InscribirAspirante" method="POST">
          <div class="form-row">
            <div class="form-group col-md-4">
                <label for="inputEmail4">Nombre Completo</label>
                <input type="text" name="nombreAspirante" required>
            </div>
            <div class="form-group col-md-4">
            <label for="inputPassword4"> Documento de identidad</label>
            <input type="number" name="documentoAspirante" required/>
            </div>
        </div>
        <div class="form-row">
            <div class="form-group col-md-4">
                <label for="inputEmail4">Correo Electronico</label>
                <input type="text" name="correoAspirante"/>
            </div>
            <div class="form-group col-md-6">
            <label for="inputPassword4">Telefono</label>
             <input type="text" name="telefonoAspirante"/>
            </div>
        </div>
        <div class="form-row">
            <div class="form-group col-md-6">
               Curso{{{listarCursosAspirante}}}
            </div>
        </div>
        <button type="submit" class="btn btn-primary">Ingresar Curso</button>
        </form>
    </div>
</div>